{
  "name": "memory",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "prettify": "prettier --write 'src/**/*.js'",
    "lint": "eslint src",
    "flow": "flow src"
  },
  "dependencies": {
    "mo-js": "0.288.2",
    "mobx": "3.3.1",
    "mobx-react": "4.3.4",
    "react": "^16.0.0",
    "react-app-rewire-mobx": "1.0.7",
    "react-app-rewired": "1.3.5",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.17"
  },
  "devDependencies": {
    "eslint": "4.11.0",
    "flow-bin": "0.59.0",
    "husky": "0.14.3",
    "lint-staged": "5.0.0",
    "prettier": "1.8.2"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "src/**/*.js": [
      "lint",
      "prettier --write",
      "git add"
    ]
  }
}
